{
  "name": "booklender",
  "version": "1.0.0",
  "description": "A book lending application",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "nodemon --delay 3 --ignore public/js/primus.js",
    "start:prod": "node server.js",
    "build": "webpack -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/j-steve-martinez/booklender.git"
  },
  "keywords": [
    "book",
    "lending",
    "reactjs",
    "nodejs",
    "javascript",
    "mongodb",
    "primus",
    "websockets"
  ],
  "author": "J. Steve Martinez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/j-steve-martinez/booklender/issues"
  },
  "homepage": "https://github.com/j-steve-martinez/booklender#readme",
  "dependencies": {
    "body-parser": "1.17.1",
    "connect-mongodb-session": "1.3.0",
    "express": "4.15.2",
    "express-session": "1.15.2",
    "google-books-search": "0.3.1",
    "if-env": "1.0.0",
    "mongoose": "4.9.3",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "primus": "7.0.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "ws": "2.2.3"
  },
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "css-loader": "0.26.1",
    "dotenv": "4.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "node-sass": "4.1.1",
    "sass-loader": "4.1.1",
    "style-loader": "0.13.1",
    "webpack": "1.14.0"
  }
}
